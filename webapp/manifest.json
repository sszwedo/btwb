{
	"_version": "1.12.0",
	"sap.app": {
		"id": "beat.beat",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponentForManifest",
			"version": "0.0.0"
		},
		"dataSources": {
			"ZKBUEXCENTER_SRV": {
				"uri": "/here/goes/your/serviceurl/",
				"type": "OData",
				"settings": {
					"localUri": "localService/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "beat.beat.view.App",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.65.6",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "beat.beat.i18n.i18n"
				}
			},
			"": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request"
				},
				"dataSource": "ZKBUEXCENTER_SRV",
				"preload": true
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewPath": "beat.beat.view",
				"controlId": "mySplitApp"
			},
			"routes": [
				{
					"name": "fullScreenRoute",
					"pattern": "",
					"target": [
						"masterTarget",
						"fullScreenTarget"
					]
				},
				{
					"name": "masterDetailRoute",
					"pattern": "NotificationHdSet/{IDWind}",
					"target": [
						"masterTarget",
						"detailTarget"
					]
				}
			],
			"targets": {
				"masterTarget": {
					"viewType": "XML",
					"transition": "slide",
					"controlAggregation": "masterPages",
					"viewId": "idMain",
					"viewName": "Main",
					"viewLevel": 1
				},
				"detailTarget": {
					"viewType": "XML",
					"transition": "slide",
					"controlAggregation": "detailPages",
					"viewId": "idEmpty",
					"viewName": "Empty",
					"viewLevel": 2
				},
				"fullScreenTarget": {
					"viewType": "XML",
					"transition": "slide",
					"controlAggregation": "detailPages",
					"viewId": "idFullScreen",
					"viewName": "FullScreen",
					"viewLevel": 1
				}
			}
		}
	},
	"sap.platform.hcp": {
		"uri": "webapp",
		"_version": "1.1.0"
	}
}