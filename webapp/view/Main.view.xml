<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:tnt="sap.tnt"
	xmlns:com="sap.suite.ui.commons" controllerName="beat.beat.controller.Main">
	<Page xmlns:cd="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" title="{i18n>win}">
		<content>
			<List id="list0" items="{path:'/NotificationHdSet',parameters:{expand:'NotifHdToItemsNav'}}" updateFinished="onFinsihed"
				selectionChange=".onSelectionChange">
				<headerToolbar>
					<OverflowToolbar>
						<SearchField id="searchField" liveChange=".onSearch" width="auto">
							<layoutData>
								<OverflowToolbarLayoutData minWidth="150px" maxWidth="240px" shrinkable="true" priority="NeverOverflow"/>
							</layoutData>
						</SearchField>
						<!--	<Button id="sortButton" press=".onOpenViewSettings" icon="sap-icon://sort" type="Transparent"/> -->
						<com:MicroProcessFlow>
							<com:content>
								<com:MicroProcessFlowItem showSeparator="false" state="Error" press="onlyFilter" key="Error"/>
								<com:MicroProcessFlowItem showSeparator="false" state="Warning" press="onlyFilter" key="Warning"/>
								<com:MicroProcessFlowItem showSeparator="false" state="Success" press="onlyFilter" key="Success"/>
							</com:content>
						</com:MicroProcessFlow>
					</OverflowToolbar>
				</headerToolbar>
				<items>
					<StandardListItem type="Navigation" title="{Windm}" description="{Type}" icon="{path: 'Type', formatter:'.formatter.imgStatus'}" id="item0"
						press=".onSelectionChange"/>
				</items>
			</List>
			<List id="list2" selectionChange=".onSelectionChangeNot" items="{path:'NotifHdToItemsNav'}" headerText="{i18n>not}" noDataText="{i18n>nod}">
				<items>
					<StandardListItem type="Navigation" title="{Qmnum}" id="item2" press=".onSelectionChangeNot" description="{Mandt}"/>
				</items>
			</List>
			<!--			<tnt:NavigationList id="navigationList" items="{path:'/NotificationHdSet',parameters:{expand:'NotifHdToItemsNav'}}"
				updateFinished="onFinsihed" selectionChange=".onSelectionChange">
				<tnt:NavigationListItem description="{Type}" text="{Windm}" icon="{path: 'Type', formatter:'.formatter.imgStatus'}" id="item0"
					hasExpander="true" press=".onSelectionChange" items="{path:'NotifHdToItemsNav'}">
					<tnt:NavigationListItem text="{Qmnum}" id="item2"></tnt:NavigationListItem>
				</tnt:NavigationListItem>
			</tnt:NavigationList>-->
			<!--			<List id="list0" items="{path:'/NotificationHdSet',parameters:{expand:'NotifHdToItemsNav'}}" updateFinished="onFinsihed"
				selectionChange=".onSelectionChange">
				<NotificationListGroup title="{Windm}" datetime="1 hour" showCloseButton="true" authorName="{Type}"
					authorPicture="{path: 'Type', formatter:'.formatter.imgStatus'}" id="item0" items="{path:'NotifHdToItemsNav'}">
					<items>
						<NotificationListItem title="{Qmnum}" description="{Matnr}" showCloseButton="true" datetime="1 hour" unread="true" priority="None"
							close="onItemClose" press="onListItemPress"/>
					</items>
				</NotificationListGroup>
			</List>-->
		</content>
	</Page>
</mvc:View>